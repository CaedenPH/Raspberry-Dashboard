  <script>
    var topScores = "<%= JSON.parse(commands.top_ten_scores); %>";
    var topNames = "<%= commands.top_ten_names; %>";

    const topTenScores = topScores.replace("[", "").replace("]", "").split(',');
    const topTenNames = topNames.replace("[", "").replace("]", "").split(',');
    
    var ctx = document.getElementById("chart-bars").getContext("2d");

    new Chart(ctx, {
      type: "bar",
      data: {
        labels: topTenNames,
        datasets: [{
          label: "Commands invoked",
          tension: 0.2,
          borderWidth: 0,
          borderRadius: 2,
          borderSkipped: false,
          backgroundColor: "rgba(255, 255, 255, .8)",
          data: topTenScores,
          maxBarThickness: 5
        }, ],
      },
      options: {
        responsive: true,
        maintainAspectRatio: false,
        plugins: {
